<?xml version="1.0"?>

<PropertyList>

  <path>eddn_light_masts2.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>
  
  <animation>
    <type>material</type> 
    <object-name>Mesh.103</object-name>
    <condition>                
      <greater-than>            
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission> 
      <red>1</red>      
      <green>1</green>  
      <blue>1</blue>
    </emission>
    <texture>eddn_light_mast_LIT.png</texture>
  </animation>

  <animation>
    <type>material</type> 
    <object-name>Mesh.103</object-name>
    <condition>                
      <less-than-equals>                
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than-equals>
    </condition>
    <emission> 
      <red>0</red>      
      <green>0</green>  
      <blue>0</blue>
    </emission>
    <texture>eddn_light_mast.png</texture>
  </animation>
  <animation>
    <type>material</type> 
    <object-name>Mesh.104</object-name>
    <condition>                
      <greater-than>            
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </greater-than>
    </condition>
    <emission> 
      <red>1</red>      
      <green>1</green>  
      <blue>1</blue>
    </emission>
    <texture>eddn_light_mast_LIT.png</texture>
  </animation>

  <animation>
    <type>material</type> 
    <object-name>Mesh.104</object-name>
    <condition>                
      <less-than-equals>                
        <property>/sim/time/sun-angle-rad</property>
        <value>1.57</value>
      </less-than-equals>
    </condition>
    <emission> 
      <red>0</red>      
      <green>0</green>  
      <blue>0</blue>
    </emission>
    <texture>eddn_light_mast.png</texture>
  </animation>
</PropertyList>
